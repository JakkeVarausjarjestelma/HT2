<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="  Valitde seura:"
        android:textSize="15dp"
        tools:ignore="MissingConstraints" />


    <Spinner
        android:id="@+id/equipmentSpinner"
        android:layout_width="289dp"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/button6"
        android:layout_width="151dp"
        android:layout_height="wrap_content"
        android:text="Hae valitsemasi seuran mukaan" />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="  Valitse Laji:"
        android:textSize="15dp"
        tools:ignore="DuplicateIds,MissingConstraints" />


    <Button
        android:id="@+id/button8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Button" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="290dp"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="  Valitse Käyttäjä:"
        android:textSize="15dp"
        tools:ignore="DuplicateIds,MissingConstraints" />


    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="290dp"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/button10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hae Valitsemasi käyttäjän mukaan" />

    <Button
        android:id="@+id/button7"
        android:layout_width="181dp"
        android:layout_height="wrap_content"
        android:background="#FC8C68"
        android:text="Button"
        android:textColor="#F1EBEB" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="388dp"
        android:text="TextView" />


</LinearLayout>
